{
  "name": "@littlemissrobot/jsonapi-client",
  "type": "module",
  "author": "Rein Van Oyen",
  "description": "A lightweight library for seamless JSON API communication, featuring a powerful query builder and intuitive models for effortless data handling.",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Little-Miss-Robot/jsonapi-client.git"
  },
  "homepage": "https://github.com/Little-Miss-Robot/jsonapi-client#readme",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "barrelsby --directory src/types --delete --include .ts,.d.ts --location all",
    "prepare": "tsup && jest",
    "build": "tsup",
    "dev": "npm run build && node ./dist/example/test.js",
    "test": "jest"
  },
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/jest": "^29.5.14",
    "barrelsby": "^2.8.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
